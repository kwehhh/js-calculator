{
  "name": "@unfocused/js-calculator",
  "description": "Show me the numbers!",
  "version": "0.0.0-alpha.0",
  "private": true,
  "main": "lib/index.js",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "scripts": {
    "build": "npm run build-standalone && npm run build-lib",
    "build-standalone": "webpack --mode=production",
    "build-lib": "webpack --mode=production --config webpack.lib.config.js",
    "start": "webpack serve --mode=development --open",
    "prepare": "npm run build",
    "watch": "webpack --watch"
  },
  "dependencies": {
    "@unfocused/spawn": "git+https://github.com/UnfocusedDrive/Spawn.git#master",
    "@unfocused/treasure-goblin": "git+https://github.com/UnfocusedDrive/treasure-goblin.git#master"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-export-default-from": "^7.14.5",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.16.7",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^5.0.0",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.4",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.2.0",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UnfocusedDrive/js-calculator.git"
